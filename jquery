colors = { "color1": "Red", "color2": "Green", 'color3': "Blue" };

$.each(colors, function(key, value) {
    $('#divSelect').append($("<option/>", {
        value: key,
        text: value
    }));
});
<div id="divSelect"></div>


function Remove_options()
{
$('#myColor')
    .empty()
    .append('<option selected="selected" value="test">White</option>');
}



