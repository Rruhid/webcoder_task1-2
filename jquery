<script>
$(document).ready(function(){
    $("#box1").toggle(
        function(){$("#1").css({"background-color": "red"});},
         function(){$("#1").css({"background-color": "blue"});},
        function(){$("#1").css({"background-color": "green"});
    });
    
});

</script>
</head>
